{
  "title": "Chimera Trading Platform",
  "tags": ["chimera", "trading", "solana"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "System Health Status",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "chimera_circuit_breaker_state",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "options": {"0": {"text": "TRIPPED", "color": "red"}}, "value": "0"},
            {"type": "value", "options": {"1": {"text": "ACTIVE", "color": "green"}}, "value": "1"}
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Queue Depth",
      "type": "gauge",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "chimera_queue_depth",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "max": 1000,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 800, "color": "yellow"},
              {"value": 900, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Active Positions",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "chimera_active_positions",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ]
    },
    {
      "id": 4,
      "title": "RPC Health",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "chimera_rpc_health",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "options": {"0": {"text": "UNHEALTHY", "color": "red"}}, "value": "0"},
            {"type": "value", "options": {"1": {"text": "HEALTHY", "color": "green"}}, "value": "1"}
          ]
        }
      }
    },
    {
      "id": 5,
      "title": "Trade Latency (p99)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(chimera_trade_latency_ms_bucket[5m]))",
          "legendFormat": "p99 Latency",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        },
        {
          "expr": "histogram_quantile(0.95, rate(chimera_trade_latency_ms_bucket[5m]))",
          "legendFormat": "p95 Latency",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        },
        {
          "expr": "histogram_quantile(0.50, rate(chimera_trade_latency_ms_bucket[5m]))",
          "legendFormat": "p50 Latency",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "id": 6,
      "title": "RPC Latency by Endpoint",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(chimera_rpc_latency_ms_bucket{endpoint=\"primary\"}[5m]))",
          "legendFormat": "Primary (p95)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        },
        {
          "expr": "histogram_quantile(0.95, rate(chimera_rpc_latency_ms_bucket{endpoint=\"fallback\"}[5m]))",
          "legendFormat": "Fallback (p95)",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "id": 7,
      "title": "Total Trades",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "chimera_total_trades",
          "legendFormat": "Total Trades",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 8,
      "title": "Queue Depth Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "chimera_queue_depth",
          "legendFormat": "Queue Depth",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "alert": {
        "conditions": [
          {
            "evaluator": {
              "params": [800],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": ["A", "5m", "now"]
            },
            "reducer": {
              "params": [],
              "type": "last"
            },
            "type": "query"
          }
        ],
        "executionErrorState": "alerting",
        "for": "5m",
        "frequency": "10s",
        "handler": 1,
        "name": "Queue Backpressure",
        "noDataState": "no_data",
        "notifications": []
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  }
}
